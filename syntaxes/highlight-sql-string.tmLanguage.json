{
    "fileTypes": [
        "py"
    ],
    "injectionSelector": "L:string.quoted.multi.python, L:string.quoted.single.python, L:meta.fstring.python",
    "patterns": [
        {
            "name": "meta.embedded.block.sql",
            "begin": "(?<!print\\s*\\(['\"])(?<![A-Za-z]+\\s)\\b((?i)(alter|analyze|attach|begin|commit|create|delete|detach|drop|explain|insert|pragma|reindex|release|rollback|savepoint|select|update|vacuum|replace|with))\\b",
            "end": "(?=(;|--|'[^\\w\\s]|\"[^\\w\\s]))",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.DML.sql"
                }
            },
            "patterns": [
                {
                    "include": "source.sql"
                }
            ]
        },
        {
            "name": "meta.embedded.block.sql",
            "begin": "\\s*(--[^\"\\n\\r{}\\\\]*)",
            "end": "(?=\\s*['\"\\n\\r])",
            "beginCaptures": {
                "0": {
                    "name": "comment.line.double-dash.sql"
                }
            },
            "patterns": [
                {
                    "include": "source.python#fstring-guts"
                },
                {
                    "match": "[^\"\\n\\r{}\\\\]*",
                    "name": "comment.line.double-dash.sql"
                }
            ]
        }
    ],
    "scopeName": "python-sql.injection"
}
